@@ -470,10 +470,18 @@ pub const Parser = struct {
         // don't accidentally issue a command when ending.
         if (self.buf_idx >= self.buf.len) {
             if (self.state != .invalid) {
-                log.warn(
-                    "OSC sequence too long (> {d}), ignoring. state={}",
-                    .{ self.buf.len, self.state },
-                );
+                const buf = self.buf[0..@min(self.buf_idx, self.buf.len)];
+                if (std.mem.startsWith(u8, buf, "1337;File=")) {
+                    log.debug(
+                        "OSC sequence too long (> {d}), ignoring. state={}",
+                        .{ self.buf.len, self.state },
+                    );
+                } else {
+                    log.warn(
+                        "OSC sequence too long (> {d}), ignoring. state={}",
+                        .{ self.buf.len, self.state },
+                    );
+                }
             }

             self.state = .invalid;
@@ -1569,10 +1577,29 @@ pub const Parser = struct {
     /// the response terminator.
     pub fn end(self: *Parser, terminator_ch: ?u8) ?Command {
         if (!self.complete) {
-            if (comptime !builtin.is_test) log.warn(
-                "invalid OSC command: {s}",
-                .{self.buf[0..self.buf_idx]},
-            );
+            if (comptime !builtin.is_test) {
+                const buf = self.buf[0..self.buf_idx];
+                const is_iterm2_file = std.mem.startsWith(u8, buf, "1337;File=");
+
+                // Avoid spamming journald with huge base64 payloads (iTerm2 inline image protocol).
+                const preview_end = if (is_iterm2_file)
+                    (std.mem.indexOfScalar(u8, buf, ':') orelse @min(buf.len, 256))
+                else
+                    @min(buf.len, 256);
+                const preview = buf[0..preview_end];
+
+                if (is_iterm2_file) {
+                    if (buf.len == preview_end)
+                        log.debug("invalid OSC command: {s}", .{preview})
+                    else
+                        log.debug("invalid OSC command (len={}): {s}...", .{ buf.len, preview });
+                } else {
+                    if (buf.len == preview_end)
+                        log.warn("invalid OSC command: {s}", .{preview})
+                    else
+                        log.warn("invalid OSC command (len={}): {s}...", .{ buf.len, preview });
+                }
+            }
             return null;
         }

